{
  "package": "es.espinr.nearby",
  "name": "Quick App Nearby",
  "versionName": "1.0.0",
  "versionCode": 1,
  "icon": "/Common/logo.png",
  "minPlatformVersion": 1070,
  "features": [
    {
      "name": "system.router"
    },
    {
      "name": "system.prompt"
    },
    {
      "name": "system.geolocation"
    },
    {
      "name": "system.fetch"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "config": {},
  "router": {
    "entry": "Intro",
    "pages": {
      "Intro": {
        "component": "intro",
        "path": "/Intro"
      },
      "Facilities": {
        "component": "facilities",
        "path": "/Facilities"
      }
    }
  },
  "display": {
    "titleBarTextColor": "#ffffff",
    "titleBarBackgroundColor": "#DF278C",
    "titleBarBackgroundOpacity": 0.7,
    "menu": true,
    "fullScreen": true,
    "pages": {}
  },
  "versionType": "debug",
  "debugable": true
}