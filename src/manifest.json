{
  "package": "com.huawei.qacodelabdemo",
  "name": "QuickApp Codelab Demo",
  "versionName": "1.0.0",
  "versionCode": 1,
  "icon": "/Common/logo.png",
  "minPlatformVersion": 1060,
  "features": [
    {
      "name": "system.configuration"
    },
    {
      "name": "system.router"
    },
    {
      "name": "system.prompt"
    },
    {
      "name": "system.shortcut"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "config": {},
  "router": {
    "entry": "Hello",
    "pages": {
      "Hello": {
        "component": "hello"
      },
      "result": {
        "component": "Resultado",
        "path": "/result"
      }
    }
  },
  "display": {
    "pages": {
      "Hello": {
        "titleBarText": "TitleBar"
      }
    }
  }
}